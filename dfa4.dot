digraph DFA {
    rankdir=LR;
    size="10,5";

    // Accepting state
    node [shape = doublecircle]; q10;
    node [shape = circle];

    start [shape=point];
    start -> q00;

    // States: qXY where X = 0/1 parity of 0's, Y = 0/1 parity of 1's
    q00 -> q10 [label="0"];
    q00 -> q01 [label="1"];

    q01 -> q11 [label="0"];
    q01 -> q00 [label="1"];

    q10 -> q00 [label="0"];
    q10 -> q11 [label="1"];

    q11 -> q01 [label="0"];
    q11 -> q10 [label="1"];
}
